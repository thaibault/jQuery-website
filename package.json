{
  "name": "website-utilities",
  "version": "1.0.221",
  "license": "CC-BY-3.0",
  "description": "extend your web app",
  "homepage": "https://torben.website/website-utilities",
  "documentationWebsite": {
    "languages": [
      "deDE",
      "enUS"
    ],
    "name": "website-utilities",
    "tagline": "extend your web app<!--deDE:erweitere deine Webanwendung-->",
    "trackingCode": "UA-40192634-5"
  },
  "keywords": [
    "jquery",
    "website",
    "utilities"
  ],
  "files": [
    "index.js",
    "index.css",
    "index.compiled.css",
    "index.sh",
    "font",
    "font.compiled"
  ],
  "main": "index.compiled.js",
  "bin": {
    "website-utilities": "index.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/website-utilities"
  },
  "scripts": {
    "build": "weboptimizer build",
    "build:export": "weboptimizer build '{__reference__: \"target:export\"}'",
    "clear": "weboptimizer clear",
    "document": "weboptimizer document",
    "lint": "weboptimizer check:type; weboptimizer lint",
    "prepare": "yarn build",
    "serve": "weboptimizer serve",
    "start": "yarn serve",
    "test": "nyc yarn test:plain",
    "test:browser": "weboptimizer test:browser",
    "test:plain": "weboptimizer test",
    "update": "yarn version --patch && yarn publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "weboptimizer build --watch"
  },
  "dependencies": {
    "clientnode": "*",
    "internationalisation": "*",
    "jquery": "*",
    "jquery.scrollto": "*",
    "spin.js": "*"
  },
  "devDependencies": {
    "@babel/plugin-transform-proto-to-assign": "*",
    "@babel/preset-env": "*",
    "babel-eslint": "*",
    "coveralls": "*",
    "css-loader": "*",
    "cssnano": "*",
    "documentation-website": "*",
    "eslint": "*",
    "eslint-config-google": "*",
    "eslint-plugin-flowtype": "*",
    "eslint-plugin-jsdoc": "*",
    "favicons-webpack-plugin": "*",
    "flow-bin": "*",
    "jsdoc": "*",
    "mini-css-extract-plugin": "*",
    "nyc": "*",
    "postcss-fontpath": "*",
    "postcss-import": "*",
    "postcss-loader": "*",
    "postcss-mixins": "*",
    "postcss-preset-env": "*",
    "postcss-sprites": "*",
    "postcss-url": "*",
    "qunit": "*",
    "style-loader": "*",
    "stylelint": "*",
    "stylelint-config-standard": "*",
    "weboptimizer": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*"
  },
  "peerDependencies": {
    "@babel/runtime": "*",
    "postcss-mixins": "*"
  },
  "sideEffects": false,
  "webOptimizer": {
    "injection": {
      "external": {
        "aliases": {
          "jQuery-scrollTo": {
            "default": "jquery.scrollto/jquery.scrollTo.min",
            "root": [
              "$",
              "fn",
              "scrollTo"
            ]
          }
        },
        "implicit": {
          "pattern": {
            "exclude": {
              "__append__": [
                {
                  "__evaluate__": "/^spin\\.js$/"
                }
              ]
            }
          }
        }
      }
    },
    "path": {
      "source": {
        "asset": {
          "font": "font/"
        }
      },
      "target": {
        "asset": {
          "font": "font.compiled/"
        }
      }
    },
    "module": {
      "aliases": {
        "#": "We need to process this file dynamically when testing different environments.",
        "clientnode/test$": "clientnode/test",
        "jQuery-scrollTo$": "jquery.scrollto/jquery.scrollTo.min"
      },
      "preprocessor": {
        "cascadingStyleSheet": {
          "additional": {
            "plugins": {
              "pre": [
                "require('postcss-mixins')"
              ]
            }
          }
        },
        "javaScript": {
          "#": "We have to pre-process \"spin.js\" since it isn't a compatible ecma script.",
          "exclude": "!(filePath.endsWith('/clientnode/test.js') || filePath.endsWith('/spin.js')) && isFilePathInDependencies(filePath)"
        }
      }
    },
    "target:export": {
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__remove__": "@babel/plugin-transform-runtime"
              }
            }
          }
        }
      }
    }
  }
}
